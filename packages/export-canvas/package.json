{
  "name": "@erbitron/loom-weave-kit-export-canvas",
  "version": "0.1.0",
  "description": "Browser canvas PNG exporter for loom weave kit",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "vitest run"
  },
  "dependencies": {
    "@erbitron/loom-weave-kit": "workspace:*"
  }
}
